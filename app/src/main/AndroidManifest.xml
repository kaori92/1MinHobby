<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.new1minhobby">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name=".activities.QuestionTypeActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.QuestionTimeActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".activities.QuestionMoneyActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".activities.HobbyResultActivity"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".activities.HobbyDetailActivity"
            android:label="@string/app_name">
        </activity>
    </application>
</manifest>
